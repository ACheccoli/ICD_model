Node CALR {

	logic = (DyingTumor);
	rate_up = @logic ? $u_CALR : 0.0;
	rate_down = @logic ? 0.0 : $d_CALR;
}

Node ATP {

	logic = (DyingTumor);
	rate_up = @logic ? $u_ATP : 0.0;
	rate_down = @logic ? 0.0 : $d_ATP;
}

Node ANXA1 {

	logic = (DyingTumor);
	rate_up = @logic ? $u_ANXA1 : 0.0;
	rate_down = @logic ? 0.0 : $d_ANXA1;
}

Node HMGB1 {

